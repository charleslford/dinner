const dinner = ['Chicken Nuggets!', 'Spaghetti and Meat Sauce!', 'Chicken Tacos!', 'General Tso!', 'Lasagna!', 'Pad Thai!', 'Hot Dogs!', 'Chili!', 'Pork Fried Rice!', 'Breakfast!', 'Chuy\'s Takeout!', 'Pizza Hut!', 'Enchiladas!', 'Freebirds Takeout!', 'North Street!', 'Spaghetti and Meatballs!']

function dinnerIdea(max) {return Math.floor(Math.random() * max)};

function whatsForDinner (){return dinner[dinnerIdea(dinner.length)]};

console.log(whatsForDinner())
